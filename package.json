{
  "name": "sanity-template-nuxt-clean",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "studio",
    "nuxt-app"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=nuxt-app\" \"npm run dev --workspace=studio\"",
    "format": "prettier --cache --write ."
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.0"
  }
}
